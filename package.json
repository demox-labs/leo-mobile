{
  "name": "leo-wallet-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "APP_VARIANT=development npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "compile": "tsc --noEmit -p . --pretty",
    "format:check": "prettier --check \"{app,src}/**/*.{js,jsx,json,md,ts,tsx}\"",
    "format:fix": "prettier --write \"{app,src}/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint app/_layout.tsx app src --ext .js,.ts,.tsx",
    "bundle:ios": "react-native bundle --entry-file App.tsx --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file App.tsx --bundle-output android/app/src/main/assets/main.jsbundle --assets-dest android/app/src/main/res",
    "prebuild": "APP_VARIANT=development npx expo prebuild",
    "prebuild:staging": "APP_VARIANT=staging npx expo prebuild",
    "prebuild:production": "APP_VARIANT=production npx expo prebuild",
    "clean": "rm -rf ios android .expo node_modules",
    "tree": "tree -I 'node_modules|ios|android|dist'",
    "doctor": "npx expo-doctor",
    "doctor:fix": "npx expo install --fix",
    "export": "npx expo export --clear --platform ios --platform android",
    "build:ios:development": "APP_VARIANT=development npx eas build --platform ios --profile development --local",
    "build:android:development": "APP_VARIANT=development npx eas build --platform android --profile development --local",
    "build:ios:staging": "APP_VARIANT=staging npx eas build --platform ios --profile staging --local --non-interactive --output ios-binary.ipa",
    "build:android:staging": "APP_VARIANT=staging npx eas build --platform android --profile staging --local --non-interactive --output android-binary.aab",
    "build:ios:production": "npx eas build --platform ios --profile production --local --non-interactive --output ios-binary.ipa",
    "build:android:production": "npx eas build --platform android --profile production --local --non-interactive --output android-binary.aab",
    "deploy:android:staging": "APP_VARIANT=staging npx eas submit --platform android --profile staging --path android-binary.aab",
    "deploy:ios:staging": "APP_VARIANT=staging npx eas submit --platform ios --profile staging --path ios-binary.ipa",
    "deploy:android:production": "npx eas submit --platform android --profile production --path android-binary.aab",
    "deploy:ios:production": "npx eas submit --platform ios --profile production --path ios-binary.ipa",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "precommit": "APP_VARIANT=development bun install --frozen-lockfile && bun doctor && bun lint && bun format:check && bun compile",
    "cleanbuild:ios": "bun clean && bun install && bun prebuild && bun ios",
    "cleanbuild:android": "bun clean && bun install && bun prebuild && bun android",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@demox-labs/aleo-wallet-adapter-base": "0.0.22",
    "@demox-labs/leo-wallet-window": "file:./leo-wallet-window",
    "@expo-google-fonts/inter": "0.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "4.6.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-clipboard/clipboard": "1.12.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/slider": "4.5.2",
    "@sentry/react-native": "~5.22.0",
    "@shopify/flash-list": "1.6.4",
    "@types/async-lock": "1.4.2",
    "@types/pako": "2.0.3",
    "async-lock": "1.4.1",
    "big-integer": "1.6.52",
    "bip39": "3.1.0",
    "constate": "3.3.2",
    "date-fns": "3.2.0",
    "effector": "23.0.0",
    "expo": "~51.0.16",
    "expo-application": "~5.9.1",
    "expo-barcode-scanner": "~13.0.1",
    "expo-build-properties": "~0.12.3",
    "expo-checkbox": "~3.0.0",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.19",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.7",
    "expo-linking": "~6.3.1",
    "expo-local-authentication": "~14.0.1",
    "expo-localization": "~15.0.3",
    "expo-router": "~3.5.17",
    "expo-splash-screen": "~0.27.5",
    "expo-sqlite": "~14.0.4",
    "expo-system-ui": "~3.0.6",
    "expo-updates": "~0.25.18",
    "expo-web-browser": "~13.0.3",
